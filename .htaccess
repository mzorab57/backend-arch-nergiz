RewriteEngine On

# OPTIONS preflight requests should be handled by PHP (config/corse.php)
# so we do not intercept them here.

# Route all requests to index.php unless the file/directory exists
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [QSA,L]